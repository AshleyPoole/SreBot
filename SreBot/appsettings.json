{
  "Logging": {
	"IncludeScopes": false,
	"Debug": {
	  "LogLevel": {
		"Default": "Information"
	  }
	},
	"Console": {
	  "LogLevel": {
		"Default": "Information"
	  }
	}
  },
  "LogConfiguration": {
	"factoryAdapter": {
	  "type": "Common.Logging.Simple.DebugLoggerFactoryAdapter, Common.Logging",
	  "arguments": {
		"level": "All",
		"showDateTime": true,
		"showLogName": true,
		"showLevel": true,
		"dateTimeFormat": "yyyy/MM/dd HH:mm:ss.fff"
	  }
	}
  },
	"Bot": {
		"newrelic:apikey": "YOUR_API_KEY_GOES_HERE",
		"slack:apiToken": "YOUR_API_KEY_GOES_HERE",
		"lbo:trustAllCerts": "true",
		"lbo:LB01": "{\"Name\": \"LB01\",\"Username\": \"loadbalancer\",\"Password\": \"loadbalancer\",\"ApiUrl\": \"https://LB01:9443/api/\",\"ApiKey\": \"YOUR_API_KEY_GOES_HERE\"}",
		"incident:mainChannel": "incidents",
		"incident:warRooms": "incident-warroom0,incident-warroom1,incident-warroom2,incident-warroom3",
		"incident:azureConnectionString": "DefaultEndpointsProtocol=https;AccountName=YOUR_ACCOUNT_NAME_GOES_HERE;AccountKey=YOUR_ACCOUNT_KEY_GOES_HERE;EndpointSuffix=core.windows.net"
		
	}
}