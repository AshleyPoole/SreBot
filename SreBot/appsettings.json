{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "LogConfiguration": {
    "factoryAdapter": {
      "type": "Common.Logging.Simple.DebugLoggerFactoryAdapter, Common.Logging",
      "arguments": {
        "level": "All",
        "showDateTime": true,
        "showLogName": true,
        "showLevel": true,
        "dateTimeFormat": "yyyy/MM/dd HH:mm:ss.fff"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=SreBot;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Bot": {
    "slack:apiToken": ""
  }
}